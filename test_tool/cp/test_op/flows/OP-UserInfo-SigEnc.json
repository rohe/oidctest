{
  "version": "1.0.0",
  "group": "Userinfo Endpoint",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "userinfo_encrypted_response_enc": "A128CBC-HS256",
          "userinfo_encrypted_response_alg": "RSA1_5",
          "userinfo_signed_response_alg": "RS256"
        },
        "check_support": {
          "ERROR": {
            "userinfo_signing_alg_values_supported": "RS256",
            "userinfo_encryption_enc_values_supported": "A128CBC-HS256",
            "userinfo_encryption_alg_values_supported": "RSA1_5"
          }
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    {
      "AccessToken": {
        "conditional_execution":{
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    },
    {
      "UserInfo": {
        "set_op_args": {
          "method": "GET",
          "authn_method": "bearer_header"
        }
      }
    }
  ],
  "usage": {
    "sig": true,
    "enc": true,
    "return_type": [
      "C",
      "IT",
      "CI",
      "CT",
      "CIT"
    ],
    "register": true,
    "extra": true
  },
  "desc": "Can provide signed and encrypted UserInfo response",
  "assert": {
    "asym-signed-userinfo": {
      "alg": "RS256"
    },
    "verify-response": {
      "response_cls": [
        "OpenIDSchema"
      ]
    },
    "encrypted-userinfo": null
  }
}