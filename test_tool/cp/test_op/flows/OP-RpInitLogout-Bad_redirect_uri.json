{
  "group": "RP-Initiated Logout",
  "note": "This test should result in the OpenID Provider displaying an error message in your user agent. You should ignore the status of this test in the test tool, since it will be incomplete. You must submit a screen shot of the error shown as part of your certification application.",
  "usage": {
    "rp_init": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Sent post_logout_redirect_uri does not match a registered post_logout_redirect_uri",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ]
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    },
    "Note",
    {
      "EndSession": {
        "set_request_args": {
          "post_logout_redirect_uri": "https://example.com/post_logout"
        },
        "set_req_args_state": null,
        "set_op_args": {
          "add_state": true
        },
        "id_token_hint": null,
        "set_expect_error": {
          "error": [
            "bad_request"
          ],
          "stop": false
        }
      }
    }
  ]
}
