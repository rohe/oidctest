{
  "group": "claims Request Parameter",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    "Registration",
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "claims": {
            "id_token": {
              "email": {
                "essential": true
              }
            },
            "userinfo": {
              "name": {
                "essential": true
              }
            }
          }
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": [
            "CIT",
            "CI",
            "C",
            "CT"
          ]
        }
      }
    },
    {
      "UserInfo": {
        "set_op_args": {
          "method": "GET",
          "authn_method": "bearer_header"
        }
      }
    }
  ],
  "usage": {
    "return_type": [
      "C",
      "IT",
      "CI",
      "CIT",
      "CT"
    ],
    "extra": true
  },
  "desc": "Supports returning different claims in ID Token and at UserInfo Endpoint",
  "assert": {
    "verify-claims": {
      "id_token": null,
      "userinfo": null
    },
    "check-http-response": null
  }
}